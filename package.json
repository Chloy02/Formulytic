{
  "name": "formulytic",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --names \"üñ•Ô∏è BACKEND,üåê FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run server\" \"npm run frontend\"",
    "server": "npm --prefix backend run dev",
    "frontend": "npm --prefix frontend run dev",
    "start": "concurrently \"npm run server:prod\" \"npm run frontend:prod\"",
    "server:prod": "cd backend && npm start",
    "frontend:prod": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && npm install --prefix frontend && npm install --prefix backend",
    "server:only": "npm --prefix backend run dev",
    "frontend:only": "npm --prefix frontend run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
